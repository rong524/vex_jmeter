        {
        	Failure = true;
        	FailureMessage = &quot; Response is missing tag &quot;+tag_name;
        	System.out.println (&quot;failure&quot;);
          return 0;	
        }
        else 
        {
          int count =0;
        	Pattern p=Pattern.compile(tag_name);
        	Matcher m=p.matcher (source);
        	while (m.find())
        	{
        		count ++;
        	}
        	System.out.println (&quot;pass&quot;);
          return count;
        
